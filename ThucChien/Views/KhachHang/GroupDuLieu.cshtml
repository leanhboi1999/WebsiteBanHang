@using ThucChien.Models;
@model List<ThucChien.Models.ThanhVien>
@{
    ViewBag.Title = "GroupDuLieu";
}

<h2>GroupDuLieu</h2>
@foreach (var GroupLTV in Model.GroupBy(n => n.LoaiThanhVien))
{
    <div>@GroupLTV.Key.TenLoai</div> // Ma
    foreach (var item in GroupLTV)
    {
        <p>@item.TaiKhoan</p>
    }
}
